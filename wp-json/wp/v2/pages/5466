		backgroundColor: "#fc9262",
		borderColor: "#fc9262",
		fill: false,
		lineTension: 0,
		pointRadius: 6,
		pointHoverRadius: 10
    }, {
		label: 'Zinco',
		data: ['2457', '2455', '2456', '2435.5', '2406', '2437.5', '2474', '2453.5', '2551', '2532.5', '2493', '2476.5', '2511.5', '2475', '2532.5'],
		backgroundColor: "#83abc5",
		borderColor: "#83abc5",
		fill: false,
		lineTension: 0,
		hidden: true,
		pointRadius: 6,
		pointHoverRadius: 10
    }, {
		label: 'Alumínio',
		data: ['2184', '2149.5', '2136.5', '2158', '2132.5', '2154', '2153', '2156.5', '2192.5', '2155.5', '2165', '2174.5', '2202', '2166.5', '2199.5'],
		backgroundColor: "#aab2bd",
		borderColor: "#aab2bd",
		fill: false,
		lineTension: 0,
		hidden: true,
		pointRadius: 6,
		pointHoverRadius: 10
    }, {
		label: 'Chumbo',
		data: ['2306', '2238.5', '2274', '2300', '2285', '2314', '2288', '2260', '2279', '2281', '2266', '2236', '2237.5', '2204', '2216'],
		backgroundColor: "#595758",
		borderColor: "#595758",
		fill: false,
		lineTension: 0,
		hidden: true,
		pointRadius: 6,
		pointHoverRadius: 10
    }, {
		label: 'Estanho',
		data: ['25600', '25560', '26400', '26100', '25335', '25550', '25205', '25200', '25650', '25600', '25550', '25650', '25800', '25650', '25325'],
		backgroundColor: "#c3b280",
