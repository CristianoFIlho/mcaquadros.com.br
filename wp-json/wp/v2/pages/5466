// COMUM
document.getElementById("meseano").addEventListener("change", myFunction);

function myFunction() {
	document.getElementById("mudagrafico").submit();
}

function convertToSlug(Text)
{
    return Text
        .toLowerCase()
        .replace(/[^\w ]+/g,'')
        .replace(/ +/g,'-')
        ;
}

// CHART.js
var ctx = document.getElementById('myChart').getContext('2d');
var myChart = new Chart(ctx, {
  type: 'line',
  options: {
	responsive: true,
	maintainAspectRatio: false,
	animation: {duration:0},
	
	tooltips: {
        callbacks: {
	        title: function(tooltipItem) {
		        if ( Number(tooltipItem[0].xLabel) < 10 ) { dia = "0"; } else { dia = "";}
                return dia + Number(tooltipItem[0].xLabel) + '/09/2023';
            }, 
            label: function(tooltipItem, data) {
	            var formato = { minimumFractionDigits: 2 }
	            if (data['datasets'][tooltipItem.datasetIndex].label == "Dólar") {
		            return "R$ " + Number(tooltipItem.yLabel).toLocaleString('pt-BR',formato);
	            } else {
		            return "USD$/t " + Number(tooltipItem.yLabel).toLocaleString('pt-BR',formato);
	            }
            }
        }
    },
	
	
	legendCallback: function(chart) {
		var text = [];
		text.push('<ul>');
		for (var i=0; i<chart.data.datasets.length; i++) {
			//console.log(chart.data.datasets[i]);
			if (i==0) {active = "active"} else {active = ""};
			text.push('<li id="legend-'+convertToSlug(chart.data.datasets[i].label)+'" class="'+active+'">');
			text.push('<span class="cor" style="background-color:' + chart.data.datasets[i].borderColor + '"></span> <span class="nome">'+ chart.data.datasets[i].label+'</span>');
			text.push('</li>');
		}
		text.push('</ul>');
		return text.join("");
	},
	legend: false,
	
	
  },
  data: {
    labels: ['01', '04', '05', '06', '08', '11', '12', '13', '14', '15', '18', '19', '20', '21', '22'],
    datasets: [{
		label: 'Cobre',
