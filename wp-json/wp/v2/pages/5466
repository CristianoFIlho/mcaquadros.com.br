document.getElementById('chartjs-legend').innerHTML = myChart.generateLegend();

document.getElementById("legend-cobre").onclick = function(){
	myChart.getDatasetMeta(0).hidden=false;
	myChart.getDatasetMeta(1).hidden=true;
	myChart.getDatasetMeta(2).hidden=true;
	myChart.getDatasetMeta(3).hidden=true;
	myChart.getDatasetMeta(4).hidden=true;
	myChart.getDatasetMeta(5).hidden=true;
	myChart.getDatasetMeta(6).hidden=true;
	myChart.update();
	document.getElementById("legend-cobre").classList.remove("active");
	document.getElementById("legend-zinco").classList.remove("active");
	document.getElementById("legend-alumnio").classList.remove("active");
	document.getElementById("legend-chumbo").classList.remove("active");
	document.getElementById("legend-estanho").classList.remove("active");
	document.getElementById("legend-nquel").classList.remove("active");
	document.getElementById("legend-dlar").classList.remove("active");
	document.getElementById("legend-cobre").classList.add("active");
}
