<script type="text/javascript">
function volume(tipo,a,b,c,d){switch(tipo){case "barra_quadrada":c=b;b=a;case "barra_retangular":case "bobina":case "chapa":return a*b*c/1000;case "tarugo":if(!a||a<=0)return-1;a=a*1+1.58;case "barra_redonda":return Math.PI*Math.pow(a/2,2)*b/1000;case "barra_hexagonal":return b*Math.sqrt(3)*Math.pow(a,2)/2000;case "bucha":b=b*1+1.58;a=a*1-1.58;return(Math.pow(b,2)-Math.pow(a,2))*0.7854*c/1000;case "tubo_redondo":return Math.PI*(a*b-Math.pow(a,2))*c/1000;case "tubo_quadrado":d=c;c=b;case "tubo_retangular":b-=a;c-=a;return(b+c)*2*a*d/1000;case "perfil_l":case "perfil_t":return(2*a*b-Math.pow(a,2))*c/1000;case "perfil_u":return(3*a*b-2*Math.pow(a,2))*c/1000;default:return-1}}
function peso(material,tipo,a,b,c,d){var materiais=new Array();materiais.Alumínio=2.7;materiais.Cobre=8.9;materiais.Latão=8.5;materiais.Bronze=9;materiais.Aço=7.85;materiais.Ferro=7.85;return volume(tipo,a,b,c,d)*materiais[material]}
function toNumber(number){number=number.replace(/,/,".");number=number.replace(/[^0-9\.]*/,"");return number}
function limpaTxt(){jQuery("#txta").val("");jQuery("#txtb").val("");jQuery("#txtc").val("");jQuery("#txtd").val("");jQuery("#divRes_valor").html("-")}
